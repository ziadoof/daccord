{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-memcache": "*",
        "ext-pdo": "*",
        "ext-zmq": "*",
        "cboden/ratchet": "^0.4.1",
        "doctrine/annotations": "^1.6",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "friendsofsymfony/message-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "~2.0",
        "jms/job-queue-bundle": "^2.0.1",
        "knplabs/knp-paginator-bundle": "^4.0",
        "mgilet/notification-bundle": "^3.1",
        "pugx/autocompleter-bundle": "^1.4",
        "ramsey/uuid": "^3.9",
        "react/zmq": "^0.4.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/dependency-injection": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.2.*",
        "symfony/http-kernel": "4.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "4.2.*",
        "symfony/property-access": "4.2.*",
        "symfony/security": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer": "4.2.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "4.2.*",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/web-profiler-bundle": "4.2.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "4.2.*",
        "twig/cssinliner-extension": "^2.0",
        "twig/cssinliner-extra": "^3.0",
        "twig/extensions": "^1.5",
        "twig/inky-extension": "^2.0",
        "twig/inky-extra": "^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "swiftmailer/swiftmailer": "^4.3 || ^5.0 || ^6.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*",
        "symfony/debug": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
