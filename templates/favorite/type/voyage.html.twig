<div class="col-sm-6 px-2 my-2">
    <div id="meetup_index" class="meetup_index">
        <a href="{{ path('voyage_show', {'id': oneFavorite.voyage.id}) }}">
            <div class="full-border border-top">
                <div class="row">
                    <div class="col-sm-8">
                        <div class="image_meetup_index">
                            <img class="" src="/assets/images/carpool/carpool.png" alt="meetup image"/>
                        </div>
                    </div>
                    <div class="col-sm-4 pt-sm-5">
                        <div class="ml-2 ml-sm-0">
                            <span class=" mt-3"><span class="blued">{% trans %} From :{% endtrans %} </span> {{ oneFavorite.voyage.stationDeparture }}</span>
                        </div>
                        <div class="ml-2 ml-sm-0">
                            <span class=" mt-3"><span class="blued">{% trans %}To :  {% endtrans %}</span> {{ oneFavorite.voyage.stationArrival }}</span>
                        </div>
                        <div class="ml-2 ml-sm-0">
                            <span class=" mt-3"><span class="blued">{% trans %} Date: {% endtrans %}</span> {{ oneFavorite.voyage.timeDeparture|date('d M Y - H:i') }}</span>
                        </div>
                        <div class="ml-2 ml-sm-0">
                            <span class=" mt-3"><span class="blued">{% trans %} With : {% endtrans %}</span> {{ oneFavorite.voyage.creator.user.firstname|title }}</span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="pt-2 border-top">
                            <div class="d-inline">
                                <span class="blued font-weight-bold pl-3">{% trans %} Voyage{% endtrans %}</span>
                            </div>
                            <div class="d-inline ">
                                {% set isFavorite = 'false' %}
                                {#data-favorite must be string not boolean#}
                                {% for favorite in app.user.getFavoritesByType('voyage')  %}
                                    {% if favorite.voyage is same as (oneFavorite.voyage) %}
                                        {% set isFavorite = 'true' %}
                                    {% endif %}
                                {% endfor %}
                                <form method="post" action="{{ url('favorite_add') }}" class="js-favorite-add  float-right mr-2 mb-2" data-object="{{ oneFavorite.voyage.id }}" data-type="voyage" data-favorite="{{ isFavorite }}">
                                    <div class="flexbox">
                                        <div class="fav-btn">
                                            <span class="fas fa-heart  favme dashicons dashicons-heart  {% if isFavorite is same as ('true') %} {{'active'}} {% endif %}"></span>
                                        </div>
                                    </div>
                                </form>
                                {% if oneFavorite.voyage.isFinish() %}
                                    <span class="badge badge-danger float-right mt-2 mr-2">{% trans %}Finish{% endtrans %}</span>
                                {% endif %}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </a>
    </div>
</div>

