<div class="container">
{{ form_start(form,{'attr':{'id':'meetup-form-new'}}) }}
        <div class="col-md-12">
            {{ form_row(form.title) }}
        </div>
        <div class="col-md-12 ">
            <div class="row">
                <div class="col-md-4">
                    {{ form_row(form.city) }}
                </div>
                <div class="col-md-4">
                    {{ form_row(form.type) }}
                </div>
                <div class="col-md-4">
                    {{ form_row(form.maxParticipants) }}
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-4">
                    <div class="">
                        {{ form_row(form.city) }}
                    </div>
                    <div class="">
                        {{ form_row(form.type) }}
                    </div>
                    <div class="">
                        {{ form_row(form.startAt) }}
                    </div>
                    <div class="">
                        {{ form_row(form.place) }}
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="">
                        {{ form_row(form.maxParticipants) }}
                    </div>
                    <div class="">
                        {{ form_row(form.endAt) }}
                    </div>
                    <div class="">
                        {{ form_row(form.address) }}
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="">
                        {{ form_row(form.image) }}
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    {{ form_row(form.description) }}
                </div>
            </div>
        <button class="btn btn-info float-right mb-4">{% trans %} Save{% endtrans %}</button>
    </div>
    {{ form_end(form) }}
</div>




