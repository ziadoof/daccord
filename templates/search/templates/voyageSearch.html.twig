{% set voyageForm = form_voyageSearchType.getForm().createView() %}
<div class="row register-form">
    <div class="col-md-12 mt-4">
        {{ form_start(voyageForm, {'attr': {'id': 'search-carpooling'}}) }}
        <div class="local-border ">
            <div class="row px-5 pt-1">
                <div class="col-md-6 ">
                    {{ form_row(voyageForm.mainDeparture) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(voyageForm.mainArrival) }}
                </div>
            </div>
            <div class="row px-5">
                <div class="col-md-6">
                    {{ form_row(voyageForm.date) }}
                </div>
                <div class="col-md-6 mt-md-5">
                    {{ form_row(voyageForm.highway,{label_attr:{'class':'checkbox-custom'}}) }}
                </div>
            </div>
        </div>
        <div class="row justify-content-center my-2">
            <div class="col-md-12">
                <input type="submit" id="valide-voyage-search" class="btn btn-info btn-block" value="{{ 'Search'|trans }}" />
            </div>
        </div>
    </div>
</div>
<a href="{{ path('search_one_city') }}" id="url-one-list" class="hidden"></a>
{{ form_end(voyageForm) }}


