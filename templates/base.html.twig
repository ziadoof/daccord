<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>{% block title %}FORDEALING{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('assets/images/logo/face.png') }}"/>
        <link rel="stylesheet" href="{{ asset('build/style.css') }}">
        <link rel="stylesheet" href="{{ asset('mmm.css') }}">
        {% block stylesheets %}
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <section id="container" >
                <section id="main-content">
                    <section class="container-fluid ">
                        <div class="wrapper ">
                            <div class="col-md-9 col-md-offset-2 col-lg-9 col-lg-offset-2 col-sm-9 col-sm-offset-3 col-xs-7 col-xs-offset-5 mt-4 mb-4 ">
                                {% block content %}

                                {% endblock %}
                            </div>
                        </div>
                    </section>
                </section>
            </section>
        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('bundles/bazingajstranslation/js/translator.min.js') }}"></script>
            <script src="{{ url('bazinga_jstranslation_js') }}"></script>
            <script src="{{ asset('build/runtime.js') }}"></script>
            <script src="{{ asset('build/app.js') }}"></script>
        {% endblock %}
    </body>
</html>
